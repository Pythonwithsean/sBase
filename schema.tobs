

$TABLE example {

  id Int key(primary)

  name String

  createdAt Date default(now)
}

$TABLE first {
  id  Int key(primary)
  second Int relation(second)
  createdAt Date default(now)
  updatedAt Date
  user Int relation(example)
  // this is a comment
}

$TABLE second {
  id  Int key(primary)
  first Int relation(first)
  createdAt Date default(now)
  updatedAt Date
  user Int relation(example)
}
